{
    "manifest_version": 3,
    "name": "Cookie Master",
    "version": "2.0",
    "description": "Export and Import cookies for the current page",
    "permissions": [
      "cookies"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  